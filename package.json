{
  "name": "reportl",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run db:empty --silent && npm run db:drop --silent && npm run build:test --silent && npm run eslint --silent",
    "start": "npm run db:up && npm run build && node server/server.js",
    "start:dev": "npm run db:up && ((sleep 3 && open http://localhost:8000) & concurrently -p '[{name}]' -c 'bgGreen.bold,bgBlue.bold' -n 'Webpack,Nodemon' 'npm run build:watch' 'npm run nodemon')",
    "build": "webpack && cp ./client/index.html ./dist/",
    "build:watch": "webpack --watch",
    "build:test": "webpack --bail && cp ./client/index.html ./dist/",
    "nodemon": "mkdir -p dist && cp ./client/index.html ./dist/ && nodemon server/server.js",
    "db:up": "knex --cwd ./server/database/ migrate:latest",
    "db:down": "knex --cwd ./server/database/ migrate:rollback",
    "db:drop": "node scripts/db_clear.js",
    "db:empty": "npm run db:drop && npm run db:up",
    "db:seed": "knex --cwd ./server/database/ seed:run",
    "db:migrate:make": "knex --cwd ./server/database/ migrate:make",
    "db:seed:make": "knex --cwd ./server/database/ seed:make",
    "db:version": "knex --cwd ./server/database/ migrate:currentVersion",
    "commit": "git-cz",
    "eslint": "eslint --ext .js --ext .jsx client server scripts",
    "lint": "npm run eslint --silent || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/closurecats/reportl.git"
  },
  "author": "Closure Cats",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/closurecats/reportl/issues"
  },
  "homepage": "https://github.com/closurecats/reportl#readme",
  "devDependencies": {
    "concurrently": "^3.1.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "formidable": "^1.0.17",
    "jsonwebtoken": "^7.2.1",
    "highcharts": "^5.0.6",
    "highcharts-drilldown": "^0.1.2",
    "knex": "^0.12.6",
    "loader": "^2.1.1",
    "lodash": "^4.17.2",
    "mysql": "^2.12.0",
    "path": "^0.12.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.8.0",
    "react-highcharts": "^11.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "webpack": "^1.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
